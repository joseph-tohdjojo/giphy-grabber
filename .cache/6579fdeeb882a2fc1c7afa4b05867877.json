"mapStateToProps","generated":{"line":152,"column":43},"source":"components/Search/Search.js","original":{"line":77,"column":23}},{"generated":{"line":152,"column":58},"source":"components/Search/Search.js","original":{"line":77,"column":15}},{"name":"mapDispatchToProps","generated":{"line":152,"column":60},"source":"components/Search/Search.js","original":{"line":77,"column":40}},{"generated":{"line":152,"column":78},"source":"components/Search/Search.js","original":{"line":77,"column":15}},{"name":"Search","generated":{"line":152,"column":80},"source":"components/Search/Search.js","original":{"line":77,"column":60}},{"generated":{"line":152,"column":86},"source":"components/Search/Search.js","original":{"line":77,"column":15}}],"sources":{"components/Search/Search.js":"import axios from 'axios'\nimport React, { Component } from 'react'\nimport { connect } from 'react-redux'\nimport PropTypes from 'prop-types'\n\nimport { fetchGiphysIfNeeded } from '../redux/action.creators'\nimport styles from './Search.scss'\n\nclass Search extends Component {\n  state = { inputVal: '' }\n\n  handleSubmit = this.handleSubmit.bind(this)\n  handlePrevSearchClick = this.handlePrevSearchClick.bind(this)\n\n  handleSubmit(e) {\n    e.preventDefault()\n    if(this.state.inputVal.length === 0) return\n\n    this.props.submitSearch(this.state.inputVal)\n    this.setState({inputVal: ''})\n  }\n\n  handlePrevSearchClick(search) {\n    return e => {\n      e.preventDefault()\n\n      this.props.submitSearch(search)\n      this.setState({inputVal: ''})\n    }\n  }\n\n  render() {\n    const searchHistory = this.props.searchHistory.map((search, i) => <li key={i}><a href=\"#\" onClick={this.handlePrevSearchClick(search)}>{search}</a></li>)\n    return (\n      <form onSubmit={this.handleSubmit} className={styles.wrapper}>\n        <input\n          className={styles.input}\n          type=\"text\"\n          onChange={e => this.setState({inputVal: e.target.value})}\n          placeholder='Search...'\n          value={this.state.inputVal}\n        />\n        <button\n          className={styles.submit}\n          type=\"submit\"\n        >\n          <img src=\"https://giphy.com/static/img/animations/search_animation_active.gif\" alt=\"Submit search\" />\n        </button>\n\n        <p className={`${styles.prevSearchesLabel} ${searchHistory.length > 0 ? '' : styles.hide}`}>Previous searches:</p>\n        <div className={styles.prevSearches}>\n          <ul>\n            { searchHistory }\n          </ul>\n        </div>\n      </form>\n    )\n  }\n}\n\nconst mapStateToProps = (state, ownProps) => ({\n  searchHistory: state.search.searchHistory,\n})\n\nconst mapDispatchToProps = (dispatch, ownProps) => ({\n  submitSearch(search) {\n    const trimmedSearch = search.trim()\n    dispatch(fetchGiphysIfNeeded(trimmedSearch))\n  },\n})\n\nSearch.propTypes = {\n  searchHistory: PropTypes.arrayOf(PropTypes.string).isRequired,\n  submitSearch: PropTypes.func.isRequired,\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Search)\n"},"lineCount":null}},"hash":"165fb090acf49f40026fd331a63ea97a","cacheData":{"env":{}}}